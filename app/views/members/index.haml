= content_for(:pagination) do
  = paginate(profiles, {search: search, views_prefix: 'pagination/'})

.row.people
  .small-12.columns
    %h1= t('.members')
    = render partial: 'members/search', locals: { search: search, circles: circles, skills: skills, interests: interests }
    = render partial: 'shared/profiles', locals: { search: search, profiles: profiles, profile_path: :member_path }
